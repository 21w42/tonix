H:=cstat
V:=cview
Y:=decode1
D:=cell a1 a2 a3 a4 cstat $V
CTX:=$H $D $Y
INIT:=$Y
INC_PATH+=../include ../../usr/include ../../usr/src/lib
include ../common.mk

cstat.sol: libdis2.sol
	@true

Y1=data
Y2=stat
Y3=message
Y4=msgdot
Y5=body
$Y/$(Y1) $Y/$(Y2) $Y/$(Y3) $Y/$(Y4) $Y/$(Y5):
	@$(TOC) runx --addr $(@D) -m $(@F) --c $(arg) | jq -r .out.buf
